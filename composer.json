{
    "name": "contao-themes-net/material-icons-inserttag",
    "type": "contao-bundle",
    "description": "Material Icons Inserttag Bundle for Contao 4",
    "license": "LGPL-3.0-or-later",
    "authors": [
      {
        "name": "Webdesign f√ºr Dresden",
        "homepage": "https://pdir.de",
        "email" : "develop@pdir.de",
        "role" : "Developer"
      }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "^4.4"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "ContaoThemesNet\\MaterialIconsInserttag\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "ContaoThemesNet\\MaterialIconsInserttag\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ContaoThemesNet\\MaterialIconsInserttag\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/contao-themes-net/material-icons-inserttag/issues",
        "source": "https://github.com/contao-themes-net/material-icons-inserttag"
    }
}
